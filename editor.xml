<application>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="2" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="Swift" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="SwiftEditorSettings">
    <option name="SHOW_ERROR_HINTS" value="true" />
    <option name="SHOW_TYPE_HINTS" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" pattern="\bremove mock\b.*" />
    <filter name="Mocks">
      <pattern index="2" />
    </filter>
  </component>
</application>